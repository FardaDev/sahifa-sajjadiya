includes:
    - vendor/larastan/larastan/extension.neon
    - vendor/nesbot/carbon/extension.neon

parameters:
    paths:
        - app
        - config
        - database
        - routes

    # Level 6 provides strict type checking while remaining practical for Laravel
    level: 6

    excludePaths:
        - vendor
        - storage
        - bootstrap/cache
